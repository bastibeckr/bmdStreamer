{
    "capture": {
        "proc": {
            "name": "ffmpeg",
            "args": [
                "-re",
                "-i", "rtmp://stream.sat1bayern.de/live/mp4:Sat1Schedule_01",
                "-c:v", "libx264",
                "-vf", "scale=720x576",
                "-c:a", "pcm_s16le",
                "-ar", "48000",
                "-f", "nut",
                "-y",
                "-"
            ]
        }
    },
    "encoder": {
        "outputOptions": [
            "-profile:v main",
            "-preset medium",
            "-crf 29",
            "-tune zerolatency",
            "-g 30",
            "-ab 96000",
            "-ar 44100",
            "-ac 2",
            "-t 10",
            "-y"
        ]
    },
    "browser": {
        "enabled": true,
        "port": 4000
    }
}